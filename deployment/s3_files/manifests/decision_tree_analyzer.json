{
    "tool": {
        "name": "analyze_clinical_decision_tree_tool",
        "description": "Specialized tool for analyzing clinical decision trees and medical workflow diagrams. ONLY use for: clinical decision trees, medical flowcharts, diagnostic algorithms, treatment pathways, clinical protocols, medical workflow diagrams, patient care pathways, and healthcare decision-making diagrams. NOT for: general diagrams, tables, handwriting, or non-medical content. If no clinical decision tree is found, returns that no clinical content was detected and suggests checking with user for appropriate tool selection.",
        "inputSchema": {
            "type": "object",
            "properties": {
                "image_path": {
                    "type": "string",
                    "description": "Absolute file path to the image containing potential clinical decision tree to analyze. Supports common image formats (PNG, JPG, etc.)"
                },
                "aws_profile": {
                    "type": "string",
                    "description": "Optional AWS profile name for Bedrock authentication. If not provided, uses default AWS credentials"
                },
                "session_id": {
                    "type": "string",
                    "description": "Runtime session ID"
                },
                "audit_mode": {
                    "type": "boolean",
                    "description": "When true, includes confidence scoring and human review flags in the analysis output"
                }
            },
            "required": [
                "image_path",
                "session_id"
            ]
        }
    },
    "analyzer": {
        "name": "decision_tree_analyzer",
        "description": "Analyzes clinical decision trees and medical workflow diagrams",
        "prompt_analyzer_prompt_base_path": "prompts",
        "prompt_files": [

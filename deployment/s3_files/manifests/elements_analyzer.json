            "elements_tasks_extraction.xml",
            "elements_format.xml",
            "elements_help.xml",
            "elements_dictionary.xml"
        ],
        "examples_path": "prompts",
        "max_examples": 0,
        "analysis_text": "document elements",
        "expected_output_tokens": 2500,
        "model_selections": {
            "primary": "global.anthropic.claude-sonnet-4-5-20250929-v1:0",
            "fallback_list": [

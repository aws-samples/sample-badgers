{
    "tool": {
        "name": "analyze_elements_tool",
        "description": "Specialized tool for extracting and identifying structural elements from document images. ONLY use for: document structure analysis, layout element identification, page component extraction, structural markup analysis, and document element classification. NOT for: content analysis, text extraction, metadata extraction, diagrams, or tables. If no structural elements are found, returns that no document elements were detected and suggests checking with user for appropriate tool selection.",
        "inputSchema": {
            "type": "object",
            "properties": {
                "image_path": {
                    "type": "string",
                    "description": "Absolute file path to the image containing document for structural elements analysis. Supports common image formats (PNG, JPG, etc.)"
                },
                "aws_profile": {
                    "type": "string",
                    "description": "Optional AWS profile name for Bedrock authentication. If not provided, uses default AWS credentials"
                },
                "session_id": {
                    "type": "string",
                    "description": "Runtime session ID"
                },
                "audit_mode": {
                    "type": "boolean",
                    "description": "When true, includes confidence scoring and human review flags in the analysis output"
                }
            },
            "required": [
                "image_path",
                "session_id"
            ]
        }
    },
    "analyzer": {
        "name": "elements_analyzer",
        "description": "Analyzes document images to extract and identify structural elements",
        "prompt_analyzer_prompt_base_path": "prompts",
        "prompt_files": [
            "../shared/gestalt_perception_foundation.xml",

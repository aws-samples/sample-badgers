<?xml version="1.0" encoding="UTF-8"?>
<response_format>
    <response>
        <description>Output a complete PDF structure tree in XML format that can be programmatically
            converted to PDF internal structure and overlaid onto the original PDF.</description>

        <structure_template>
            <StructTreeRoot Type="/StructTreeRoot">
                <Metadata>
                    <Title>{DOCUMENT_TITLE}</Title>
                    <Author>{AUTHOR_NAMES}</Author>
                    <Subject>{SUBJECT_DESCRIPTION}</Subject>
                    <Keywords>{COMMA_SEPARATED_KEYWORDS}</Keywords>
                    <Language>{LANGUAGE_CODE}</Language>
                </Metadata>

                <Document>
                    <!-- Artifacts: Non-content elements -->
                    <Artifact Type="Pagination">
                        <Content>{RUNNING_HEADER_TEXT}</Content>
                        <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                    </Artifact>

                    <!-- Content structure begins -->
                    <Sect>
                        <H1>
                            <Content>{DOCUMENT_TITLE_TEXT}</Content>
                            <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                        </H1>

                        <P Role="Authors">
                            <Content>{AUTHOR_NAMES_WITH_SUPERSCRIPTS}</Content>
                            <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            <Reference Type="Footnote" RefID="{NOTE_ID}">{SUPERSCRIPT_NUMBER}</Reference>
                        </P>

                        <!-- Affiliations as footnotes -->
                        <Note Type="Footnote" ID="{NOTE_ID}">
                            <Label>{SUPERSCRIPT_NUMBER}</Label>
                            <Content>{AFFILIATION_TEXT}</Content>
                            <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                        </Note>

                        <!-- Keywords section -->
                        <Aside Type="Keywords">
                            <H3>
                                <Content>Key words:</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </H3>
                            <P>
                                <Content>{KEYWORD_LIST}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </P>
                        </Aside>

                        <!-- Main content sections -->
                        <Sect>
                            <H2>
                                <Content>{SECTION_HEADING}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </H2>

                            <P>
                                <Content>{PARAGRAPH_TEXT}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                <Reference Type="Citation" RefID="{CITATION_ID}">{CITATION_NUMBER}</Reference>
                            </P>

                            <P>
                                <Content>{PARAGRAPH_WITH_URL}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                <Link Href="{URL}">
                                    <Content>{VISIBLE_LINK_TEXT}</Content>
                                </Link>
                            </P>

                            <!-- Subsection -->
                            <Sect>
                                <H3>
                                    <Content>{SUBSECTION_HEADING}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </H3>

                                <P>
                                    <Content>{SUBSECTION_PARAGRAPH}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </P>
                            </Sect>
                        </Sect>

                        <!-- Example: List structure -->
                        <L>
                            <LI>
                                <Lbl>โข</Lbl>
                                <LBody>
                                    <P>
                                        <Content>{LIST_ITEM_TEXT}</Content>
                                        <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                    </P>
                                </LBody>
                            </LI>
                        </L>

                        <!-- Example: Table structure -->
                        <Table>
                            <TR>
                                <TH Scope="column">
                                    <Content>{HEADER_1}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </TH>
                                <TH Scope="column">
                                    <Content>{HEADER_2}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </TH>
                            </TR>
                            <TR>
                                <TD>
                                    <Content>{DATA_1}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </TD>
                                <TD>
                                    <Content>{DATA_2}</Content>
                                    <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                                </TD>
                            </TR>
                        </Table>

                        <!-- Example: Figure with alt text -->
                        <Figure Alt="{DESCRIPTIVE_ALT_TEXT}" ID="{FIGURE_ID}">
                            <Content>[Image: {BRIEF_DESCRIPTION}]</Content>
                            <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                        </Figure>

                        <Caption>
                            <Content>{FIGURE_CAPTION}</Content>
                            <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                        </Caption>

                        <!-- Footer artifacts -->
                        <Artifact Type="Pagination">
                            <P Role="Copyright">
                                <Content>{COPYRIGHT_TEXT}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </P>

                            <P Role="VolumeInfo">
                                <Content>{VOLUME_ISSUE_INFO}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </P>

                            <P Role="DOI">
                                <Content>{DOI_URL}</Content>
                                <Link Href="{DOI_URL}">
                                    <Content>{DOI_URL}</Content>
                                </Link>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </P>

                            <P Role="PageNumber">
                                <Content>{PAGE_NUMBER}</Content>
                                <MCR PageRef="{PAGE_NUMBER}" ContentRef="{OBJECT_ID}" />
                            </P>
                        </Artifact>
                    </Sect>
                </Document>
            </StructTreeRoot>
        </structure_template>

        <element_examples>
            <example type="H1">
                <H1>
                    <Content>ESMO Clinical Practice Guideline interim update on new targeted
                        therapies</Content>
                    <MCR PageRef="1" ContentRef="Obj3" />
                </H1>
            </example>

            <example type="H2">
                <H2>
                    <Content>INTRODUCTION</Content>
                    <MCR PageRef="1" ContentRef="Obj8" />
                </H2>
            </example>

            <example type="P">
                <P>
                    <Content>The following ESMO Clinical Practice Guideline (CPG) has been recently
                        updated with new treatment recommendations.</Content>
                    <MCR PageRef="1" ContentRef="Obj9" />
                </P>
            </example>

            <example type="P_with_citation">
                <P>
                    <Content>Recent studies have shown significant improvements in patient outcomes.</Content>
                    <MCR PageRef="1" ContentRef="Obj12" />
                    <Reference Type="Citation" RefID="Ref1">ยน</Reference>
                </P>
            </example>

            <example type="Note">
                <Note Type="Footnote" ID="Affil1">
                    <Label>ยน</Label>
                    <Content>Department I for Internal Medicine, Center for Integrated Oncology,
                        University of Cologne, Cologne, Germany</Content>
                    <MCR PageRef="1" ContentRef="Obj5" />
                </Note>
            </example>

            <example type="Aside">
                <Aside Type="Keywords">
                    <H3>
                        <Content>Key words:</Content>
                        <MCR PageRef="1" ContentRef="Obj7a" />
                    </H3>
                    <P>
                        <Content>BCL2 inhibitor, BTK inhibitor, chronic lymphocytic leukaemia,
                            guideline update</Content>
                        <MCR PageRef="1" ContentRef="Obj7b" />
                    </P>
                </Aside>
            </example>

            <example type="Link">
                <P>
                    <Content>View the original CPG here: </Content>
                    <Link
                        Href="https://www.esmo.org/guidelines/guidelines-by-topic/haematological-malignancies/chronic-lymphocytic-leukaemia">
                        <Content>https://www.esmo.org/guidelines/...</Content>
                    </Link>
                    <MCR PageRef="1" ContentRef="Obj10" />
                </P>
            </example>

            <example type="Figure">
                <Figure
                    Alt="ESMO logo showing the organization's branding with text 'Good Science, Better Medicine, Best Practice'"
                    ID="Fig_Logo1">
                    <Content>[Image: ESMO Logo]</Content>
                    <MCR PageRef="1" ContentRef="Obj2" />
                </Figure>
            </example>

            <example type="Artifact_Pagination">
                <Artifact Type="Pagination">
                    <Content>ARTICLE IN PRESS</Content>
                    <MCR PageRef="1" ContentRef="Obj1" />
                </Artifact>
            </example>

            <example type="Sect_with_subsection">
                <Sect>
                    <H2>
                        <Content>MANAGEMENT OF ADVANCED DISEASE</Content>
                        <MCR PageRef="1" ContentRef="Obj17" />
                    </H2>

                    <Sect>
                        <H3>
                            <Content>First-line treatment</Content>
                            <MCR PageRef="1" ContentRef="Obj20" />
                        </H3>

                        <P>
                            <Content>Different treatment strategies are available...</Content>
                            <MCR PageRef="1" ContentRef="Obj21" />
                        </P>
                    </Sect>
                </Sect>
            </example>
        </element_examples>

        <attribute_specifications>
            <attribute name="Type">
                <usage>Specifies the PDF structure type</usage>
                <values>/StructTreeRoot, /Document, /Sect, /H1-H6, /P, /L, /LI, /Table, /TR, /TH,
                    /TD, /Figure, /Note, /Link, /Artifact</values>
            </attribute>

            <attribute name="Alt">
                <usage>Alternative text for figures and non-text content</usage>
                <required_for>Figure elements</required_for>
                <guidelines>Describe content and purpose, be concise but informative</guidelines>
            </attribute>

            <attribute name="ID">
                <usage>Unique identifier for cross-referencing</usage>
                <required_for>Elements that are referenced (Notes, Figures, Tables)</required_for>
                <format>Alphanumeric, must be unique within document</format>
            </attribute>

            <attribute name="RefID">
                <usage>Points to the ID of a referenced element</usage>
                <used_in>Reference elements</used_in>
                <must_match>An existing ID value</must_match>
            </attribute>

            <attribute name="Href">
                <usage>URL or destination for hyperlinks</usage>
                <used_in>Link elements</used_in>
                <format>Full URL (https://...) or internal destination</format>
            </attribute>

            <attribute name="Role">
                <usage>Clarifies semantic role when structure type is ambiguous</usage>
                <examples>Role="Authors", Role="Copyright", Role="PageNumber"</examples>
            </attribute>

            <attribute name="Scope">
                <usage>Specifies scope of table header cells</usage>
                <used_in>TH elements</used_in>
                <values>"row" or "column"</values>
            </attribute>

            <attribute name="PageRef">
                <usage>Page number where content appears</usage>
                <used_in>MCR (Marked Content Reference) elements</used_in>
                <format>Integer page number (1-based)</format>
            </attribute>

            <attribute name="ContentRef">
                <usage>Reference to PDF object containing visual content</usage>
                <used_in>MCR (Marked Content Reference) elements</used_in>
                <format>Object identifier (e.g., "Obj123")</format>
            </attribute>
        </attribute_specifications>

        <output_requirements>
            <requirement>Output must be valid, well-formed XML</requirement>
            <requirement>Use proper indentation for readability (2 or 4 spaces)</requirement>
            <requirement>Include XML declaration: &lt;?xml version="1.0" encoding="UTF-8"?&gt;</requirement>
            <requirement>Escape special XML characters (&amp; &lt; &gt; &quot; &apos;)</requirement>
            <requirement>Include comments to explain complex structures or decisions</requirement>
            <requirement>Ensure all opening tags have matching closing tags</requirement>
            <requirement>Maintain consistent attribute quoting (use double quotes)</requirement>
        </output_requirements>
    </response>
</response_format>